<%- include('partials/header') %>
<div class="container mt-5">



    <div class="col-sm-12">
      <div class="card social-block">

        <div class="card-body">
          <!-- <h1>Sign-Up</h1> -->
          <div class="socials goog">
            <a class="btn  btn-google" href="/auth/google" role="button">
              <i class="fab fa-google s g"><span>   Sign-Up With Google</span></i>

            </a>
          </div>
         <div class="socials fb">
           <a class="btn  btn-facebook" href="/auth/facebook" role="button">
             <i class="fab fa-facebook s f"><span>   Sign-Up With Facebook</span></i>


           </a>

         </div>

          <p class="or">OR</p>
          <hr>
          <h3>Sign-Up</h3>
          <form action="/register" method="POST">
            <div class="form-group">
              <!-- <label for="email">Email</label> -->
              <input type="email" class="form-control" name="username" placeholder="email">
            </div>
            <div class="form-group">
              <!-- <label for="password">Password</label> -->
              <input type="password" class="form-control" name="password" placeholder="password">
            </div>
            <button type="submit" class="btn btn-dark register">Register</button>
          </form>


        </div>
      </div>
    </div>


  </div>


<%- include('partials/footer') %>
